<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="splashScreenVM" type="com.hotelbooking.app.modules.splashscreen.data.viewmodel.SplashScreenVM"/>
    </data>
    <LinearLayout android:id="@+id/linearSplashScreen"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_900">
        <ImageView android:id="@+id/imageGroup158"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_165pxh"
            android:layout_width="@dimen/_177pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_group158"
            tools:ignore="ContentDescription" tools:src="@drawable/img_group158"/>
    </LinearLayout>
</layout>
